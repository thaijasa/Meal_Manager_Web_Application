 <!-- “SJSU CMPE 226 Spring 2019 TEAM2” -->
 
<!DOCTYPE html>
<html lang="en-US">
<head>
    <% include head %>
   
    
     <% include css/dietician_format %>
    
</head>

<body>
<header>
    <% include dietician_loggedin_header %>
</header>

<div class="dietician_main_display">
	<h2>All Customers</h2> 
	<% if (customers_present=='Y' ) { %>
	     <form name="dieticianviewcustomers" id="dieticianviewcustomers" method="POST" action="/d_customer_diet_plan_list">
			<table id="dietician_view_table">
   						<tr>
   					 		<th> &nbsp;&nbsp; SelectCustomer &nbsp;&nbsp;</th> 
                     		<th> &nbsp;&nbsp; CustomerId &nbsp;&nbsp;</th> 
                     		<th> &nbsp;&nbsp; CustomerName &nbsp;&nbsp;</th> 
                     		<th> &nbsp;&nbsp; DietStartDate &nbsp;&nbsp;</th> 
                     		<th> &nbsp;&nbsp; DietRequestDate &nbsp;&nbsp;</th>
                		</tr>							
                    	<% for(var i=0; i < query_results.length; i++) { %>
                    		<tr>
                        		<td><input type="radio" name="cust_id" value= <%=  query_results[i].cust_id %> checked> </td>
                        		<td>&nbsp;&nbsp; <%=  query_results[i].cust_id %> &nbsp;&nbsp;</td>
                        		
                        		<td>&nbsp;&nbsp; <%=  query_results[i].cust_name %> &nbsp;&nbsp;</td>
                        		<td>&nbsp;&nbsp; <%=  query_results[i].diet_start_date.toDateString()  %>&nbsp;&nbsp;</td>
                         		<td>&nbsp;&nbsp; <%=  query_results[i].new_diet_plan_request_date.toDateString()  %> &nbsp;&nbsp;</td>
                           </tr>
                    	<% } %>  
               
             </table> 
                	<button type="submit" class="submit_form_button" >View Diet Plan</button>
          </form>                        
<% } %>
<%  if (customers_present=='N' ) { %>
    	<div class="no_data_information_message">
        	<p>  Customers will choose you soon. Thank you for your patience! </p>
    	</div>
<% } %>
</div>

            
</body>
</html>